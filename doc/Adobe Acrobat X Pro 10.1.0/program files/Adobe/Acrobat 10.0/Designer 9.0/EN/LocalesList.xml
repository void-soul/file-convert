<patterns>
    <custom type="text" desc="Postal Code (Canada)" pattern="A9A 9A9"/>
    <custom type="text" desc="Postal Code (United Kingdom)" pattern="A9A 9A9|AA9A 9A9"/>
    <custom type="text" desc="Zip Code (USA)" pattern="text{99999-9999}|text{99999}"/>
    <custom type="text" desc="Telephone (USA - Canada)" pattern="text{'('999')' 999-9999}"/>
    <custom type="text" desc="Email - alphanumeric" pattern="text{OOOOOOOO'@example.com'}"/>
    <custom type="text" desc="EIN" pattern="text{99-9999999}"/>
    <custom type="num" pattern="$z,zz9.99"/>
    <custom type="num" pattern="$z,zz9.99CR"/>
    <custom type="num" pattern="$z,zz9.99DB"/>
    <custom type="num" pattern="($z,zz9.99)"/>
   <custom type="pwd" desc="Password 8-character" pattern="XXXXXXXX"/>
    <custom type="pwd" desc="Password 8-alphanumeric" pattern="OOOOOOOO"/>
</patterns>

